pragma solidity ^0.8.19;

contract Counter {
    uint256 public count;

    // get current count
    function getCount() public view returns (uint256) {
        return count;
    }

    // increase count by 1
    function increaseCount() public {
        count += 1;
    }

    // decrese count by 1
    function decreaseCount() public {
        count -= 1;
    }
}
